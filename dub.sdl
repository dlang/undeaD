name "undead"
description "Obsolete Phobos modules, back from the dead"
homepage "https://github.com/dlang/undeaD"
authors "various"
license "BSL-1.0"
targetType "library"
targetPath "bin"
buildType "unittest" {
    buildOptions "debugMode" "debugInfo" "unittests" "deprecationErrors" "warningsAsErrors"
}
excludedSourceFiles "src/undead/xml.d"
dependency "undead:xml" version="*"
subPackage {
    name "xml"
    targetType "library"
    sourceFiles "src/undead/xml.d"
}
